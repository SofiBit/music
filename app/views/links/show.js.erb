$('#show_div').empty();
$("<%= escape_javascript(render 'show') %>").appendTo("#show_div");

<% if current_user %>
  getStars(<%= Track.find_track(@result).id %>, <%= current_user.id %>);
<% end %>

function getStars(track_id, user_id) {
  $.ajax({
    url: 'http://localhost:3000/assessment.json',
    type: 'get',
    data: `track_id=${track_id}&user_id=${user_id}`,
    success: function(response) {
      if(response.status == 'success') {
        starsBehavior(response.stars);
      } else {
        starsBehavior(stars = 0);
      }
    }
  });
}
